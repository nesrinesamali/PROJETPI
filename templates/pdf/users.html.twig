<!DOCTYPE html>
<html lang="en">
	<head>
		<meta charset="UTF-8">
		<meta http-equiv="X-UA-Compatible" content="IE=edge">
		<meta name="viewport" content="width=device-width, initial-scale=1.0">
		<title>User List</title>
	</head>
	<body>
		<h1>User List</h1>
		<table>
			<thead>
				<tr>
					<th>ID</th>
					<th>Name</th>
					<th>Email</th>
					<!-- Add more table headers if needed -->
				</tr>
			</thead>
			<tbody>
				{% for user in users %}
					<tr>
						<td>{{ user.id }}</td>
						<td>{{ user.nom }}</td>
						<td>{{ user.email }}</td>
						<!-- Add more table cells if needed -->
					</tr>
				{% endfor %}
			</tbody>
		</table>
	</body>
</html>
